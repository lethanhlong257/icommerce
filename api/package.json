{
  "name": "icommerce",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "engines": {
    "node": "^14.25.1"
  },
  "scripts": {
    "dev": "DEBUG=icommerce:* nodemon --exec babel-node --presets @babel/preset-env src/index.js",
    "build": "npx babel --presets @babel/preset-env src --out-dir build",
    "start": "npx babel --presets @babel/preset-env src --out-dir build && DEBUG=icommerce:* node ./build/index.js",
    "update-sub": "git submodule sync; git submodule update --remote; cd src/services/core-services; npm install"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "route-decorators": "^0.2.2",
    "sequelize": "^6.6.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-export-default-from": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-spread": "^7.14.6",
    "@babel/preset-env": "^7.15.6",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.13",
    "sequelize-cli": "^6.2.0"
  }
}
